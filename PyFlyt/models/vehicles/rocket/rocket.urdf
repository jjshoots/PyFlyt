      <child link="fin_neg_z"/>
    </joint>


    <link name="leg_1">
      <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.00"/>
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      </inertial>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <box size="0.05 0.05 0.5"/>
        </geometry>
      </collision>
    </link>
    <joint name="leg_1_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0.0 -2.4 0.35"/>
      <parent link="base_link"/>
      <child link="leg_1"/>
    </joint>


    <link name="leg_2">
      <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.00"/>
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      </inertial>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <box size="0.05 0.05 0.5"/>
        </geometry>
      </collision>
    </link>
    <joint name="leg_2_joint" type="fixed">
      <origin rpy="0 4.188 0" xyz="-0.3031 -2.4 -0.175"/>
      <parent link="base_link"/>
      <child link="leg_2"/>
    </joint>


    <link name="leg_3">
      <inertial>
       <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.00"/>
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      </inertial>
      <collision>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <box size="0.05 0.05 0.5"/>
