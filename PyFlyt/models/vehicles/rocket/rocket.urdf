    <joint name="fin_pos_y_joint" type="fixed">
      <origin rpy="0 0 0" xyz="0 2.051 0.35"/>
      <parent link="base_link"/>
      <child link="fin_pos_z"/>
    </joint>


    <link name="fin_neg_z">
      <inertial>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <mass value="0.05"/>
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      </inertial>
      <collision>
